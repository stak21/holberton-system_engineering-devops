sudo apt-get install ufw
sudo ufw status
sudo ufw disable
sudo ufw enable
sudo ufw default deny incoming
sudo ufw default allow outgoing
sudo ufw allow 80/tcp
sudo ufw allow 443/tcp
sudo ufw allow 22/tcp
sudo ufw reload
sudo ufw allow ssh
sudo ufw allow www
sudo ufw deny 80
sudo ufw remove deny 80
sudo ufw reset
